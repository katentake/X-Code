class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        #input: 99
        #output: 100
        # 使用 reversed(range(...)) 从数组的最后一个索引（个位）开始向前遍历
        for i in reversed(range(len(digits))):
            if digits[i] == 9:
                digits[i] = 0
            else:
                digits[i] += 1
                return digits

        digits[i] = 1
        digits.append(0)
        return digits