class Solution:
    def isValidBST(self, root: Optional[TreeNode]) -> bool:

        def valid(self, min, max, root):    
            if not root:
                return True
            if not (min < root.val < max):
                return Flase

        return self.valid(root.left, min, root.val) and self.valid(root.right, root.val, max)
        
