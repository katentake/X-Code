import random

class Solution:

    def __init__(self):
        # 存储实际的数值
        self.nums = []
        # 存储 数值 -> 索引 的映射
        self.pos_map = {}

    def insert(self, val: int) -> bool:
        if val in self.pos_map:
            return False
        # 将新元素加到末尾，并在哈希表中记录索引
        self.pos_map[val] = len(self.nums)
        self.nums.append(val)
        return True

    def remove(self, val: int) -> bool:
        if val not in self.pos_map:
            return False
        # 1. 获取要删除元素的索引和数组最后一个元素
        idx_to_remove = self.pos_map[val]
        last_val = self.nums[-1]
        # 2. 将最后一个元素移动到要删除的位置
        self.nums[idx_to_remove] = last_val
        self.pos_map[last_val] = idx_to_remove
        # 3. 删除最后一个元素及其索引记录
        self.nums.pop()
        del self.pos_map[val]
        return True

    def getRandom(self) -> int:
        # 利用 random.choice 在数组中 O(1) 随机选取
        return random.choice(self.nums)