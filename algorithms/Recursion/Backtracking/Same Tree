class Solution:
    def isSameTree(self, p: Optional[TreeNode], q: Optional[TreeNode]) -> bool: 
        if not p and not q:
            return True
        if not p or not q or p.val != q.val: #如果其中一个为空，或者值不相等，说明不相等
            return False
        return self.isSameTree(p.left, q.left) and self.isSameTree(p.right, q.right) #递归比较左子树和右子树