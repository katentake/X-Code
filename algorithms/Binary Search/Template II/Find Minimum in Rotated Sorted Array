class Solution:
    def findMin(self, nums: List[int]) -> int:
        if not nums:
            return
        
        left, right = 0, len(nums)-1

        # 使用 Template II: left < right
        while left <= right:
            mid = (left + right) // 2

            if nums[mid] >= nums[left]:
                res = min(res, nums[left])
                left = mid + 1
            else:
                res = min(res, nums[mid])
                right = mid -1

        return res


class Solution:
    def findMin(self, nums: List[int]) -> int:
        if not nums:
            return

        left, right = 0 ,len(nums)-1

        while left < right:
            mid = (left + right) // 2
            if nums[mid] < nums[right]:
                left = mid + 1
            else:
                right = mid  #nums[mid] <= nums[right]

        return nums[left]