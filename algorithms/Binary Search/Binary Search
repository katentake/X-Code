class Solution:
    def search(self, nums: List[int], target: int) -> int:
        left, right = 0, len(nums)-1 

        #find the mid index number
        while left <= right:
            mid = (left + right) // 2

            if nums[mid] == target:
                return mid # 找到目标，返回索引
            elif nums[mid] < target:
                left = mid +1 #目标在右半部分
            else:
                right = mid -1 #目标在左半部分

        return -1 # 未找到目标
