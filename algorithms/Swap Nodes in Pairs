class Solution:
    def swapPairs(self, head: Optional[ListNode]) -> Optional[ListNode]:
        if not head or not head.next:
            return head

"""
1 -> 2 -> 3 -> 4
head head.next

"""
        first_node = head
        second_node = head.next

        first_node.next = swapPairs(second_node.next)

        second_node.next = first_node

    return second_node